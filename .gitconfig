[remote "kamikat"]
    url = https://github.com/kamikat/bilibili-get.git
    fetch = +refs/heads/*:refs/remotes/kamikat/*
[remote "origin"]
    url = git@github.com:huangyingw/kamikat_bilibili-get.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = kamikat
    target = kamikat/master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/git/tools/kamikat/bilibili-get
